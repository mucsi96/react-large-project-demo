{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "ui-lib": "^1.0.0"
  },
  "devDependencies": {
    "dev-tools": "1.0.0",
    "typescript": "4.1.3",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "cpy-cli": "3.1.1",
    "cross-env": "7.0.3"
  },
  "scripts": {
    "lint": "dev-tools lint",
    "check-types": "dev-tools check-types",
    "test": "dev-tools test",
    "test:update": "dev-tools test -u",
    "test:watch": "dev-tools test --watch",
    "start": "dev-tools start",
    "build": "dev-tools build",
    "qa": "npm-run-all lint check-types test build",
    "build:app": "yarn build && cpy build ../../dist/app-prod && cross-env USE_MOCK_API=true yarn build && cpy build ../../dist/app-mock"
  }
}
