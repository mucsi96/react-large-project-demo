{
  "name": "dev-tools",
  "version": "1.0.0",
  "license": "MIT",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": "./bin.js",
  "files": [
    "bin.js",
    "eslint.js",
    "intTest.js",
    "nodeLib",
    "lib",
    "config"
  ],
  "engines": {
    "node": ">= 10.0.0"
  },
  "scripts": {
    "lint": "eslint src nodeSrc --max-warnings 0",
    "check-types": "tsc && tsc --project nodeSrc",
    "build:lib": "rollup --config config/rollup.config.js",
    "build:nodeLib": "tsc --project nodeSrc --noEmit false --declaration --outDir nodeLib",
    "build": "del-cli lib nodeLib && yarn build:lib && yarn build:nodeLib",
    "prepack": "npm run build",
    "update:dependencies": "npx npm-check-updates -u",
    "qa": "npm-run-all lint check-types",
    "prepare": "yarn build"
  },
  "dependencies": {
    "@storybook/addon-essentials": "6.1.14",
    "@storybook/addon-links": "6.1.14",
    "@storybook/addons": "6.1.14",
    "@storybook/react": "6.1.14",
    "@storybook/preset-create-react-app": "3.1.5",
    "@types/enzyme": "3.10.8",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.22",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "enzyme-to-json": "3.6.1",
    "typescript": "4.1.3",
    "del": "6.0.0",
    "del-cli": "3.0.1",
    "@rollup/plugin-commonjs": "17.0.0",
    "@rollup/plugin-node-resolve": "11.1.0",
    "@rollup/plugin-typescript": "8.1.0",
    "@rollup/plugin-url": "6.0.0",
    "@svgr/rollup": "5.5.0",
    "rollup": "2.37.1",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "4.0.0",
    "rollup-plugin-visualizer": "4.2.0",
    "postcss": "8.1.8",
    "tslib": "2.1.0",
    "react-app-rewired": "2.1.8",
    "react-scripts": "4.0.1",
    "babel-jest": "26.6.3",
    "babel-loader": "8.1.0",
    "eslint-config-react-app": "6.0.0",
    "eslint": "7.18.0",
    "node-sass": "4.14.1",
    "path-to-regexp": "6.2.0",
    "@cucumber/cucumber": "^7.0.0",
    "@types/puppeteer": "^5.4.2",
    "expect": "^26.6.2",
    "jest-snapshot": "^26.6.2",
    "puppeteer": "^5.5.0",
    "minimist": "1.2.5",
    "lerna": "3.22.1",
    "react": "16.13.1",
    "@types/react": "17.0.0",
    "ts-node": "9.1.1"
  }
}
